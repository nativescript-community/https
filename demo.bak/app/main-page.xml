<Page loaded="onLoaded" unloaded="onUnloaded">

    <Page.actionBar>
        <ActionBar title="HTTPS Playground"></ActionBar>
    </Page.actionBar>

    <GridLayout rows="*">
        <StackLayout>
            <Button text="Test HTTPS" tap="testit" />
            <Button text="Enable SSL" tap="enableSSL" />
            <Button text="Disable SSL" tap="disableSSL" />
        </StackLayout>
    </GridLayout>

</Page>